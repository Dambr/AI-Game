{
	"size": { 
		// размер карты по вертикале
		"vertical": 100, 
		// размер карты по горизонтали
		"horizontal": 100
	},
	// описание сегмента как части карты
	"segments": [
		{
			// id-шник
			"id" : 1,
			// размеры сегменты
			"size": {
				// размер по вертикали
				"vertical": 10,
				// размер по горизонтали
				"horizontal": 10
			},
			// то, чем заполнен сегмент (ландшафт, препятствие, отряд и т.д.)
			"fill": [
				{
					// id объекта
					"id": 1,
					// тип объекта
					"type": ""
				}
			]
		}
	],
	// описание объектов как часть карты
	"objects": [
		{
			// id-шник 
			"id" : 1,
			// тип объекта. перечень допустимых типов подлежит обсуждению
			"type": "",
			// название
			"name": ""
		}
	]
}